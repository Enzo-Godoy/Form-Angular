{
  "name": "form-angular",
  "version": "1.0.0",
  "description": "Proyecto integrador AngularJS",
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server ./app -a localhost -p 8000 -c-1"
  },
  "author": "Enzo",
  "license": "ISC",
  "dependencies": {
    "angular": ">=1.8.0",
    "angular-css-injector": "^1.0.4",
    "bootstrap": ">=3.4.1",
    "braces": ">=2.3.1",
    "cpx": "^1.5.0",
    "ecstatic": ">=3.3.2",
    "http-server": "^0.12.3",
    "minimist": ">=0.2.1",
    "mixin-deep": ">=1.3.2",
    "qs": ">=6.0.4",
    "set-value": ">=2.0.1"
  }
}
